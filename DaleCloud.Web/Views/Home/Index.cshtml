<script>
    // 利用按钮，对整个页面进行全屏或退出操作
    $(".fullscreen").click(function () {
        if (screenfull.enabled) {
            // 仅仅只进行全屏而不退出全屏可以调用
            // screenfull.request() 方法。
            screenfull.toggle();
        } else {
            // Ignore or do something else
        }
    });

    function btn_revisepassword() {
        var UserCode = '@ViewData["UserCode"]';
        if (UserCode != null) {
            var keyValue = '@ViewData["UserId"]';
            var Account = '@ViewData["UserCode"]';
            var RealName = '@ViewData["UserName"]';
            $.modalOpen({
                id: "RevisePassword",
                title: '重置密码',
                url: '/SystemManage/User/RevisePassword?keyValue=' + keyValue + "&account=" + escape(Account) + '&realName=' + escape(RealName),
                width: "450px",
                height: "260px",
                callBack: function (iframeId) {
                    top.frames[iframeId].submitForm();
                }
            });
        }
        else {
            $.modalMsg("未选中数据", "warning");
        }
    }

    function SetSkin() {
        var width = $(window).width() - 330;
        layer.open({
            id:"skinset",
            type: 1,
            title: "皮肤设置",
            fix: false,
            offset: ['125px','600px'],
            content: $('#LAY_adminPopupTheme'), //这里content是一个DOM，注意：最好该元素要存放在body最外层，否则可能被其它的相对元素所影响

        });
    }
</script>